diff --git a/19_keyinput.c b/19_keyinput.c
new file mode 100644
index 0000000..e25ec1f
--- /dev/null
+++ b/19_keyinput.c
@@ -0,0 +1,143 @@
+#include <linux/module.h>
+#include <linux/kernel.h> 
+#include <linux/init.h>
+#include <linux/fs.h> 
+#include <linux/uaccess.h> 
+#include <linux/errno.h>
+#include <linux/slab.h>
+#include <linux/string.h>
+#include <linux/stat.h>   
+#include <linux/cdev.h>
+#include <linux/device.h>
+#include <linux/of.h>
+#include <linux/of_gpio.h>
+#include <linux/gpio.h>
+#include <linux/types.h>
+#include <linux/platform_device.h>
+#include <linux/input.h>
+#include <linux/interrupt.h>
+
+
+/* * 定義私有數據結構 
+ * 建議：不要使用全域變數，將所有設備相關的變數封裝在這裡
+ */
+struct key_input_data {
+    int irq;
+    struct input_dev *input;
+    struct gpio_desc *gpiod;
+    unsigned int key_code;
+};
+
+/* * 中斷處理函式 (ISR)
+ * 當按鍵按下時，核心會跳進這裡。
+ * 實作重點：
+ * 1. 讀取目前的 GPIO 電位狀態。
+ * 2. 使用 input_report_key() 上報狀態 (0為按下, 1為放開)。
+ * 3. 使用 input_sync() 告訴系統上報完畢。
+ */
+static irqreturn_t key_input_isr(int irq, void *dev_id)
+{
+    struct key_input_data *priv = dev_id;
+    int state;
+
+    state = gpiod_get_value(priv->gpiod);
+    input_report_key(priv->input, priv->key_code, !state);
+    input_sync(priv->input);
+
+    return IRQ_HANDLED;
+}
+
+/* * Probe 函式 (設備匹配時執行)
+ * 這是驅動程式的心臟：
+ */
+static int key_input_probe(struct platform_device *pdev)
+{
+    struct device *dev = &pdev->dev;
+    struct key_input_data *priv;
+    int ret;
+
+    pr_info("key-input: Probing started...\n");
+
+    /* 1. 分配私有數據結構記憶體 (使用 devm_kzalloc) */
+    priv = devm_kzalloc(dev, sizeof(*priv), GFP_KERNEL);
+    if (!priv) return -ENOMEM;
+
+    /* 2. 從 Device Tree 獲取 GPIO 描述字 (使用 devm_gpiod_get) */
+    priv->gpiod = devm_gpiod_get(dev, "key0", GPIOD_IN);
+    if (IS_ERR(priv->gpiod)) {
+        dev_err(dev, "Failed to get GPIO\n");
+        return PTR_ERR(priv->gpiod);
+    }
+
+    gpiod_set_debounce(priv->gpiod, 20000); // 設定 20ms 的防彈跳時間 (單位為微秒)
+
+    /* 3. 配置 Input Device 對象 (使用 devm_input_allocate_device) */
+    priv->input = devm_input_allocate_device(dev);
+    if (!priv->input) {
+        dev_err(dev, "Failed to allocate input device\n");
+        return -ENOMEM;
+    }
+
+    /* 4. 設定 Input Device 的能力 (EV_KEY, BTN_0 等) */
+    priv->key_code = KEY_ENTER; 
+    priv->input->name = "hsiao-key-input";
+    priv->input->id.bustype = BUS_HOST;
+    priv->input->id.vendor  = 0x000A;
+    priv->input->id.product = 0x5678;
+
+    input_set_capability(priv->input, EV_KEY, priv->key_code);
+
+    /* 5. 獲取中斷號碼並註冊中斷處理函式 (gpiod_to_irq + devm_request_irq) */
+    /* 注意：傳遞 priv 作為 dev_id 以便在 ISR 中存取數據 */
+    priv->irq = gpiod_to_irq(priv->gpiod);
+    if (priv->irq < 0) {
+        dev_err(dev, "Failed to get IRQ number\n");
+        return priv->irq;
+    }
+
+    ret = devm_request_irq(dev, priv->irq, key_input_isr,
+                           IRQF_TRIGGER_RISING | IRQF_TRIGGER_FALLING,
+                           "inputkey", priv);
+    if (ret) {
+        dev_err(dev, "Failed to request IRQ\n");
+        return ret;
+    }
+
+    /* 6. 正式註冊 Input Device (input_register_device) */
+    ret = input_register_device(priv->input);
+    if (ret) {
+        dev_err(dev, "Failed to register input device\n");
+        return ret;
+    }
+
+    /* 將私有數據存入 platform_device 以便移除時使用 */
+    platform_set_drvdata(pdev, priv);
+
+    pr_info("key-input: Probe success! IRQ line: %d\n", priv->irq);
+
+    return 0;
+}
+
+/* * 第四部分：驅動基礎設定與匹配表
+ */
+static const struct of_device_id key_input_ids[] = {
+    { .compatible = "hsiao,inputkey" },
+    { }
+};
+MODULE_DEVICE_TABLE(of, key_input_ids);
+
+static struct platform_driver key_input_driver = {
+    .probe = key_input_probe,
+    .driver = {
+        .name = "key_input_driver",
+        .of_match_table = key_input_ids,
+    },
+};
+
+/* Module Definition */
+module_platform_driver(key_input_driver);
+
+MODULE_AUTHOR("Leo <littlepig602@gmail.com>");
+MODULE_LICENSE("GPL");
+MODULE_DESCRIPTION("A step-by-step Input Driver Template");
+
